{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {                     
    "quotes":[2, "double"],
    "linebreak-style": 0,
    "eqeqeq": "off",
    "curly": "error",
    "treatUndefinedAsUnspecified": true              
  }
}